<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dhitoshi.xfrs.huixiaobao">

    <!-- 网络访问 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 拨号权限 -->
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.Theme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.Chat" />
        <activity android:name=".view.AddClient" />
        <activity android:name=".view.ClientInfo" />
        <activity android:name=".view.AddSpend" />
        <activity android:name=".view.AddVisit" />
        <activity android:name=".view.AddRelation" />
        <activity android:name=".view.AddGift" />
        <activity android:name=".view.AddMeeting" />
        <activity android:name=".view.Resource" />
        <activity android:name=".view.Remind" />
        <activity android:name=".view.Contact" />
        <activity android:name=".view.Query" />
        <activity android:name=".view.Setting" />
        <activity android:name=".view.Login"></activity>
    </application>

</manifest>